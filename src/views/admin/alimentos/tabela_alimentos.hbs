{{#each rows}}
<tr>
    <td>{{this.id}}</td>
    <td>{{this.nome}}</td>
    <td>{{this.proteina}}</td>
    <td>{{this.carbs}}</td>
    <td>{{this.gordura}}</td>
    <td>{{this.calorias}}</td>
    <td>
         <a href="/admin/editar_alimento/{{this.id}}" type="button" class="btn btn-dark btn-small"><i class="bi bi-pencil-square"></i> Editar</a>
         <a href="/admin/apagar_alimento/{{this.id}}" type="button" class="btn btn-dark btn-small"><i class="bi bi-trash3"></i> Apagar</a>
        
    </td>
</tr>
{{/each}}