{{#each rows}}
<tr>
    <td>{{this.id}}</td>
    <td>{{this.exercicio}}</td>
    <td>{{this.equipamento}}</td>
    <td>{{this.dificuldade}}</td>
    <td>{{this.musculo}}</td>
    <td>
         <a href="/admin/editar_exercicio/{{this.id}}" type="button" class="btn btn-dark btn-small"><i class="bi bi-pencil-square"></i> Editar</a>
        <button type="button" class="btn btn-dark btn-small"><i class="bi bi-trash3"></i> Apagar</button>
    </td>
</tr>
{{/each}}